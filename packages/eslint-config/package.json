{
  "name": "@nozomiishii/eslint-config",
  "version": "0.6.8",
  "description": "Nozomi's Recommended eslint config",
  "keywords": [
    "eslint",
    "lint",
    "config"
  ],
  "homepage": "https://github.com/nozomiishii/configs/blob/main/packages/eslint-config/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/nozomiishii/configs.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": "Nozomi Ishii",
  "type": "module",
  "main": "eslint.config.ts",
  "bin": "bin/cli.sh",
  "scripts": {
    "build": "pnpx @eslint/config-inspector build",
    "check:ts": "tsc",
    "dev": "pnpm typegen && eslint --inspect-config",
    "eslint": "eslint --max-warnings=0",
    "postinstall": "pnpm typegen",
    "link": "pnpm link --global",
    "lint": "pnpm eslint .",
    "lint:fix": "pnpm eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "start": "pnpx serve .eslint-config-inspector",
    "typegen": "node --import @swc-node/register/esm-register scripts/typegen.ts"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint-react/eslint-plugin": "2.4.0",
    "@eslint/config-inspector": "1.4.2",
    "@eslint/js": "9.39.2",
    "@next/eslint-plugin-next": "16.1.1",
    "@stylistic/eslint-plugin": "5.6.1",
    "@swc-node/register": "1.11.1",
    "@types/node": "24.10.4",
    "@vitest/eslint-plugin": "1.6.3",
    "eslint": "9.39.2",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-better-tailwindcss": "3.8.0",
    "eslint-plugin-de-morgan": "2.0.0",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsdoc": "61.5.0",
    "eslint-plugin-jsonc": "2.21.0",
    "eslint-plugin-jsx-a11y-x": "0.1.1",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-package-json": "0.85.0",
    "eslint-plugin-perfectionist": "5.0.0",
    "eslint-plugin-playwright": "2.4.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "eslint-plugin-regexp": "2.10.0",
    "eslint-plugin-storybook": "10.1.10",
    "eslint-plugin-tsdoc": "0.5.0",
    "eslint-plugin-unicorn": "62.0.0",
    "eslint-typegen": "2.3.0",
    "globals": "16.5.0",
    "jsonc-eslint-parser": "2.4.2",
    "tailwindcss": "4.1.18",
    "typescript-eslint": "8.50.1"
  },
  "devDependencies": {
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.26.2",
  "publishConfig": {
    "access": "public"
  }
}

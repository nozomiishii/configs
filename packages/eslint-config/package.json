{
  "name": "@nozomiishii/eslint-config",
  "version": "0.6.4",
  "description": "Nozomi's Recommended eslint config",
  "keywords": [
    "eslint",
    "lint",
    "config"
  ],
  "homepage": "https://github.com/nozomiishii/configs/blob/main/packages/eslint-config/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/nozomiishii/configs.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": "Nozomi Ishii",
  "type": "module",
  "main": "eslint.config.ts",
  "bin": "bin/cli.sh",
  "scripts": {
    "build": "pnpx @eslint/config-inspector build",
    "check:ts": "tsc",
    "dev": "eslint --inspect-config",
    "eslint": "eslint --max-warnings=0",
    "postinstall": "pnpm typegen",
    "link": "pnpm link --global",
    "lint": "pnpm eslint .",
    "lint:fix": "pnpm eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "start": "pnpx serve .eslint-config-inspector",
    "typegen": "node --import @swc-node/register/esm-register scripts/typegen.ts"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint-react/eslint-plugin": "1.53.1",
    "@eslint/config-inspector": "1.3.0",
    "@eslint/js": "9.36.0",
    "@next/eslint-plugin-next": "15.5.4",
    "@swc-node/register": "1.11.1",
    "@types/node": "22.18.6",
    "@vitest/eslint-plugin": "1.3.10",
    "eslint": "9.36.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-better-tailwindcss": "3.7.9",
    "eslint-plugin-de-morgan": "1.3.1",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsdoc": "60.3.1",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-jsx-a11y-x": "0.1.1",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-package-json": "0.56.3",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.21",
    "eslint-plugin-regexp": "2.10.0",
    "eslint-plugin-storybook": "9.1.8",
    "eslint-plugin-tsdoc": "0.4.0",
    "eslint-plugin-unicorn": "61.0.2",
    "eslint-typegen": "2.3.0",
    "globals": "16.4.0",
    "jsonc-eslint-parser": "2.4.1",
    "tailwindcss": "4.1.13",
    "typescript-eslint": "8.44.1"
  },
  "devDependencies": {
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "eslint": "^9.36.0",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.17.1",
  "publishConfig": {
    "access": "public"
  }
}
